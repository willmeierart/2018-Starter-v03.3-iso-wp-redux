{
  parser: babel-eslint,
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "semi":0,
    "spaced-comment":0,
    "no-console":0,
    "function-paren-newline":0
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ]
}
